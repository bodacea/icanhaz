<script type="text/javascript" src="../menu/menu.js"></script>
<script type="text/javascript" language="javascript">
function Page(i, OtherSubSector, Status){
	var val ="?initPage="+ i +"&OtherSubSector=" + OtherSubSector + "&status="+ Status ; 
	Ajax_UpdatePanel("report_listing_by_sector.asp", val,"report_listing_by_sector",true);
}

function selectStatus()
{
	ById("cmbStatus").value = '0'
}

function search(){

	var submitValue = ""
   	if (ById("OtherSubSector").value != 0) {
		submitValue ='?OtherSubSector='+ ById("OtherSubSector").value;
   	}

	if (ById('cmbStatus').value != 0) {
		submitValue +='&status='+ ById('cmbStatus').value;
	}

   	Ajax_UpdatePanel('report_listing_by_sector.asp', submitValue,'report_listing_by_sector',true);

}
</script>
</head>

